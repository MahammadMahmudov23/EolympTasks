#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    int count = 0;

    for (int num = 100; num <= 999; ++num) {
        int digit1 = num / 100;        
        int digit2 = (num / 10) % 10; 
        int digit3 = num % 10;         

        if (digit1 + digit2 + digit3 == n) {
            count++;
        }
    }

    printf("%d\n", count);

    for (int num = 100; num <= 999; ++num) {
        int digit1 = num / 100;        
        int digit2 = (num / 10) % 10;  
        int digit3 = num % 10;        

        if (digit1 + digit2 + digit3 == n) {
            printf("%d\n", num);
        }
    }

    return 0;
}
