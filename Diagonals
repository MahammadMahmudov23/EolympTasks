#include<iostream>
#include<stdio.h>
using namespace std;

int i,j,n,a,b;
int arr[510][510];


int main()
{
    cin>>n;
    for(i=0;i<n;i++)
        for(j=0;j<n;j++)
        {
            cin>>arr[i][j];
        }
    a=b=0;
    for(i=0;i<n;i++)
        for(j=0;j<n;j++)
        {
            if(i==j) a+=arr[i][j];
             if(i+j==n-1) b+=arr[i][j];
        }
    cout<<a<<" "<<b;
    
    return 0;
}
